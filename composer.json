{
    "name": "pravin/textareafield",
    "description": "A Laravel Nova field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1"
    },
    "autoload": {
        "psr-4": {
            "Indianic\\Textareafield\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Indianic\\Textareafield\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
